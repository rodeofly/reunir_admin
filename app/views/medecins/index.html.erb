<h1>Listing medecins</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @medecins.each do |medecin| %>
  <tr>
    <td><%= medecin.name %></td>
    <td><%= link_to 'Show', medecin %></td>
    <td><%= link_to 'Edit', edit_medecin_path(medecin) %></td>
    <td><%= link_to 'Destroy', medecin, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Medecin', new_medecin_path %>
